<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="bean.Visite" table="visites">
        <id name="idVisite" column="id_visite" type="int" >
            <generator class="identity" />
        </id>
        
        <property name="codeMed" column="code_med" type="string" not-null="true" />
        <property name="codePat" column="code_pat" type="string" not-null="true" />
        <property name="dateVisite" column="date_visite" type="date" not-null="true" />
    </class>
    
</hibernate-mapping>
